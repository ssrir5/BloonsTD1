module dart_creation(input logic Clk, input logic [19:0] dartfileStart, input logic [19:0] dartfileDest, input logic monk_ready, input logic [63:0] blooncount,
                     output logic [19:0] dartfile);

logic [9:0] init_x_disp, init_y_disp;
// logic monk_ready_temp;



// always_ff @ (posedge monk_ready)
// begin
//     monk_ready_temp <= 1'b1;
//     else 
// end






always_ff @ (posedge Clk)
begin
    //y2-y1 = m(x2-x1) y2 is final dest
    if(monk_ready == 1)
    begin
        init_x_disp <= dartfileDest[19:10] - dartfileStart[19:10];
        init_y_disp <= dartfileDest[9:0] - dartfileStart[9:0];
        dartfile <= dartfileStart;
    end

    if(dartfileStart != 0 && dartfile != dartfileDest )
    begin
        if(blooncount  % 33333333 == 0)
        begin
            dartfile[19:10] <= dartfile[19:10] + (init_x_disp  >> 4);

            dartfile[9:0] <= dartfile[9:0] + (init_y_disp >> 4);
        end
    end



end


endmodule
